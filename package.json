{
  "name": "hp-ui",
  "version": "0.1.58",
  "private": false,
  "main": "lib/hp-ui.min.js",
  "module": "es/index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "dev": "npm run build:file && webpack-serve --config build/webpack.dev.js",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:file": "node build/build-entry.js",
    "build:components": "node build/build-components.js --color",
    "build:lib": "node build/build-lib.js",
    "build:hpui": "cross-env NODE_ENV=production webpack --color --config build/webpack.build.js && cross-env NODE_ENV=production webpack -p --color --config build/webpack.build.js",
    "test": "jest",
    "release": "node build/release.js"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "element-ui": "^2.4.5",
    "font-awesome-webpack": "0.0.5-beta.2",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "uppercamelcase": "^3.0.0",
    "vue": "^2.5.17"
  },
  "keywords": [
    "vue",
    "hp-ui"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@vue/cli-plugin-babel": "^3.0.0-rc.5",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.5",
    "@vue/cli-service": "^3.0.0-rc.5",
    "@vue/eslint-config-prettier": "^3.0.0-rc.5",
    "@vue/server-test-utils": "^1.0.0-beta.23",
    "@vue/test-utils": "^1.0.0-beta.23",
    "FontAwesome-webpack": "0.0.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "browserify": "^16.2.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "font-awesome": "^4.7.0",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "jest": "^23.4.2",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^4.9.2",
    "progress-bar-webpack-plugin": "^1.11.0",
    "pug": "^2.0.3",
    "pug-filters": "^3.1.0",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.2",
    "signale": "^1.2.1",
    "style-loader": "^0.21.0",
    "transform-runtime": "0.0.0",
    "vue-jest": "^2.6.0",
    "vue-sfc-compiler": "0.0.8",
    "vue-template-compiler": "^2.5.17",
    "vueify": "^9.4.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "vue/attribute-hyphenation": [
        "error",
        "always"
      ],
      "vue/html-quotes": [
        "error",
        "double"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
